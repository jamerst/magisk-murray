    <!-- Control whether the always on display mode is enabled by default. This value will be used
         during initialization when the setting is still null. false by default. -->
    <bool name="config_dozeAlwaysOnEnabled">true</bool>

    <!-- Default LED on time for notification LED in milliseconds. 200 by default. -->
    <integer name="config_defaultNotificationLedOn">500</integer>
    <!-- Default LED off time for notification LED in milliseconds. 5800 by default. -->
    <integer name="config_defaultNotificationLedOff">2000</integer>

    <!-- Default value for led color when battery is low on charge. 0xFFFF0000 by default. -->
    <integer name="config_notificationsBatteryLowARGB">0xFF0C0000</integer>
    <!-- Default value for led color when battery is medium charged. 0xFFFF3000 by default. -->
    <integer name="config_notificationsBatteryMediumARGB">0xFF0C0200</integer>
    <!-- Default value for led color when battery is fully charged. 0xFF00FF00 by default. -->
    <integer name="config_notificationsBatteryFullARGB">0xFF000C00</integer>
